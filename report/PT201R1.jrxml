	<queryString>
		<![CDATA[SELECT P.RECEIPT_NO,P.PATIENTID,P.PATIENTNAME, P.DATE_TEXT_EN, P.DATE_TEXT_TH , P.TOTAL_AMOUNT, T.PAY_DETAIL_TYPE_NAME, D.AMOUNT
FROM TB_T_PAYMENT P
INNER JOIN TB_T_PAYMENT_DETAIL D ON P.PAYMENTID = D.PAYMENTID
INNER JOIN TB_M_PAYMENT_DETAIL_TYPE T ON D.PAY_DETAIL_TYPE = T.ID
WHERE P.RECEIPT_NO=$P{in_receipt_no}
--GROUP BY P.RECEIPT_NO,P.PATIENTID,P.PATIENTNAME, P.DATE_TEXT_EN, P.DATE_TEXT_TH , P.TOTAL_AMOUNT]]>
